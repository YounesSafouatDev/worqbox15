<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="worqbox_view_company_form_inherit_social_media" model="ir.ui.view">
        <field name="name">worqbox.res.company.form.inherit.social.media</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="social_media.view_company_form_inherit_social_media"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='social_instagram']" position="after">
                <field name="social_tiktok"/>
            </xpath>
        </field>
    </record>

</odoo>
