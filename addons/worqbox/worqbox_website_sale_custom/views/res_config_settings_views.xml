<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="worbox_res_config_settings_view_form" model="ir.ui.view">
        <field name="name">worqbox.res.config.settings.view.form.inherit.website</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="website.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='social_media_settings']/div[hasclass('o_setting_right_pane')]/div[hasclass('content-group')]" position="inside">
                <div class="row">
                    <label for="social_tiktok" string="TikTok" class="col-md-3 o_light_label"/>
                    <field name="social_tiktok"/>
                </div>
            </xpath>
        </field>
    </record>

</odoo>